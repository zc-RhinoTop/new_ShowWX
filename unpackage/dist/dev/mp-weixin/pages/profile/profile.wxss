button::after {
  border: none;
}
.profile {
  height: 100%;
  background-color: #f5f5f5;
}
.profile .header {
    width: 100%;
    height: 260rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: start;
    -webkit-justify-content: flex-start;
            justify-content: flex-start;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    background: url(data:image/png;base64,) no-repeat center/100%;
}
.profile .header .unlogin {
      margin: 0 40rpx;
      width: 156rpx;
      height: 156rpx;
      border-radius: 50%;
      background: #F7F7F7 url(data:image/png;base64,) no-repeat center;
      background-size: 140rpx 140rpx;
}
.profile .header .login {
      margin: 0 40rpx;
      width: 156rpx;
      height: 156rpx;
      border-radius: 50%;
      overflow: hidden;
}
.profile .header .login > image {
        width: 100%;
        height: 100%;
}
.profile .header .info {
      color: #fff;
}
.profile .header .info .name {
        padding-bottom: 20rpx;
}
.profile .header .login-text {
      width: 172rpx;
      height: 60rpx;
      font-size: 26rpx;
      border-radius: 40rpx;
      border: 1px solid #fff;
      text-align: center;
      line-height: 60rpx;
      padding: 0;
      background-color: #FB8629;
      color: #fff;
}
.profile .logistics {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    width: 100%;
    height: 152rpx;
    border-bottom: 40rpx solid #F5F5F5;
    background-color: #fff;
}
.profile .logistics > view {
      width: 25%;
      text-align: center;
}
.profile .logistics image {
      width: 60rpx;
      height: 60rpx;
      display: block;
      margin: 10rpx auto;
}
.profile .logistics text {
      font-size: 26rpx;
}
.profile .profile-list {
    height: 98rpx;
    border-top: 1px solid #f5f5f5;
    background-color: #fff;
    padding-left: 40rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.profile .profile-list image {
      width: 60rpx;
      height: 60rpx;
}
.profile .profile-list text {
      padding-left: 20rpx;
}
.profile .sign-out {
    background-color: #fff;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    border-top: 20rpx solid #F5F5F5;
}

